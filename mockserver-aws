# if the node module does not exist yet, installed it
if [ -d node_modules ] ; then 
    echo "Node modules exist"; 
else 
    npm install moment; 
    npm install tunnel;
    npm install aws-sdk; 
    npm install child_process;
    npm install prompt-autocompletion; 
    npm install request; 
    npm install shorteners; 
    npm install inquirer; 
    npm install commander; 
fi 


#!/usr/bin/env node
var main = require('./mock.js')
main({})
